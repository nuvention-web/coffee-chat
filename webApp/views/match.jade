html
	head
		link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css")
		link(href='https://fonts.googleapis.com/css?family=Source+Sans+Pro' rel='stylesheet' type='text/css')
		link(rel="stylesheet" href="/style/index.css")
	body
		include ./includes/header.jade
		h2(class="center text-orange") Coffee Chat Connected!
		div(class="row container valign-wrapper")
			div(class="col s12 m4 l4")
				- var person = user
				include ./includes/card.jade
			div(class="col s12 m4 l4 valign")
				div(class="center")
					img(class="responsive-img" src="http://d9hhrg4mnvzow.cloudfront.net/womensilab.com/coffeechat2/1a1a3105-mint-template-speakericon.png")
					div(class="coffee-chat-grey")
						p Tuesday, February 2, 2016
						p 2-3pm
						p via Skype
			div(class="col s12 m4 l4")
				- var person = match
				include ./includes/card.jade
		div(class="section coffee-chat-orange")
			div(class="row container white-text")
				div(class="col s12")
					h4 More About #{match.name}
				div(class="col s12 m4 l3")
					img(class="col s12 m12 l12 circle responsive-img" src=match.image)
				div(class="col s12 m4 l3")
					h5=match.quote
				div(class="col s12 m4 l3")
					h5(class="text-underline") Work Experience:
					div Product Manager and Business Intelligence, Aereo
					div Special Operations, Warby Parker 
					div Marketing, Quirky
					div Special Customer Operations, Simon Schuster 
					div Associate, Triage Consulting Group
				div(class="col s12 m4 l3")
					p
					h5 Education:
					div MBA from NYU Stern Business School
					div University of California San Diego
					p
					div Interests: 
					div #strategy #marketing #tech #SF #49ers #dogs #consulting #productmanager
